    "@babel/preset-env": "^7.8.6",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "edge-js": "^13.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "jasmine": "^3.5.0",
    "jsdoc-to-markdown": "^5.0.3",
    "jsondiffpatch": "^0.4.1",
    "karma": "^4.4.1",
    "karma-browserify": "^7.0.0",
