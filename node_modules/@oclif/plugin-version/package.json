{
  "name": "@oclif/plugin-version",
  "description": "A command that shows the CLI version",
  "version": "2.0.9",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-version/issues",
  "dependencies": {
    "@oclif/core": "^3.14.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@oclif/plugin-help": "5.2.20",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4.3.10",
    "commitlint": "^17.8.1",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "fancy-test": "^3",
    "husky": "^8",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "oclif": "^4",
    "prettier": "^3.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "oclif.manifest.json",
    "oclif.lock",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/plugin-version",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "exports": "./lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "oclif/plugin-version",
  "scripts": {
    "build": "shx rm -rf lib && tsc",
    "clean": "shx rm -f oclif.manifest.json oclif.lock",
    "commitlint": "commitlint",
    "lint": "eslint . --ext .ts",
    "postpublish": "yarn run clean",
    "posttest": "yarn lint",
    "prepare": "husky install && yarn build",
    "prepublishOnly": "yarn run build && oclif lock && oclif manifest . && oclif readme",
    "pretest": "yarn build --noEmit && tsc -p test --noEmit",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "type": "module"
}
