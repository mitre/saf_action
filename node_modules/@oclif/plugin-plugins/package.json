{
  "name": "@oclif/plugin-plugins",
  "description": "plugins plugin for oclif",
  "version": "4.1.19",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-plugins/issues",
  "dependencies": {
    "@oclif/core": "^3.10.2",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "npm": "10.3.0",
    "npm-run-path": "^4.0.1",
    "semver": "^7.5.4",
    "shelljs": "^0.8.5",
    "validate-npm-package-name": "^5.0.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@oclif/plugin-help": "^6",
    "@oclif/prettier-config": "^0.2.1",
    "@types/chai": "^4.3.11",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "@types/semver": "^7.5.6",
    "@types/shelljs": "^0.8.15",
    "@types/sinon": "^10.0.19",
    "@types/validate-npm-package-name": "^4.0.2",
    "chai": "^4.4.1",
    "commitlint": "^17.8.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.35",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mocha": "^10.2.0",
    "oclif": "^4.1.3",
    "prettier": "^3.1.1",
    "shx": "^0.3.4",
    "sinon": "^16.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "oclif.manifest.json",
    "/lib",
    "/oclif.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-plugins",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "update": "./lib/hooks/update.js"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "aliases": {
      "aliasme": "@oclif/plugin-test-esm-1"
    },
    "bin": "mycli",
    "flexibleTaxonomy": true,
    "scope": "oclif",
    "topicSeparator": " "
  },
  "repository": "oclif/plugin-plugins",
  "scripts": {
    "build": "shx rm -rf lib && tsc",
    "clean": "shx rm -f oclif.manifest.json oclif.lock",
    "commitlint": "commitlint",
    "lint": "eslint . --ext .ts",
    "postpublish": "yarn run clean",
    "posttest": "yarn lint",
    "prepare": "husky install && yarn build",
    "prepublishOnly": "yarn run build && oclif lock && oclif manifest . && oclif readme",
    "pretest": "yarn build && tsc -p test --noEmit",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:integration": "mocha \"test/**/*.integration.ts\"",
    "test:integration:sf": "mocha \"test/**/sf.integration.ts\"",
    "test:integration:install": "mocha \"test/**/install.integration.ts\"",
    "test:integration:link": "mocha \"test/**/link.integration.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module",
  "types": "lib/index.d.ts"
}
